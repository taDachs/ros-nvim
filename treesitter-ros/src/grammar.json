{
  "name": "ros",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "separator"
          },
          {
            "type": "SYMBOL",
            "name": "field_definition"
          }
        ]
      }
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment_char"
        },
        {
          "type": "SYMBOL",
          "name": "comment_string"
        }
      ]
    },
    "comment_char": {
      "type": "STRING",
      "value": "#"
    },
    "comment_string": {
      "type": "PATTERN",
      "value": ".*"
    },
    "separator": {
      "type": "STRING",
      "value": "---"
    },
    "field_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field_type"
        },
        {
          "type": "SYMBOL",
          "name": "field_name"
        }
      ]
    },
    "field_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "builtin_type"
        },
        {
          "type": "SYMBOL",
          "name": "custom_type"
        },
        {
          "type": "SYMBOL",
          "name": "array_type"
        }
      ]
    },
    "builtin_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "bool"
        },
        {
          "type": "STRING",
          "value": "int8"
        },
        {
          "type": "STRING",
          "value": "uint8"
        },
        {
          "type": "STRING",
          "value": "int16"
        },
        {
          "type": "STRING",
          "value": "uint16"
        },
        {
          "type": "STRING",
          "value": "int32"
        },
        {
          "type": "STRING",
          "value": "uint32"
        },
        {
          "type": "STRING",
          "value": "int64"
        },
        {
          "type": "STRING",
          "value": "uint64"
        },
        {
          "type": "STRING",
          "value": "float32"
        },
        {
          "type": "STRING",
          "value": "float64"
        },
        {
          "type": "STRING",
          "value": "string"
        },
        {
          "type": "STRING",
          "value": "time"
        },
        {
          "type": "STRING",
          "value": "duration"
        }
      ]
    },
    "custom_type": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "array_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "builtin_type"
            },
            {
              "type": "SYMBOL",
              "name": "custom_type"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "field_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "const_definition"
        }
      ]
    },
    "const_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "const_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "const_value"
        }
      ]
    },
    "const_name": {
      "type": "PATTERN",
      "value": "[A-Z_]+"
    },
    "const_value": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9\\.]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9/_]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

